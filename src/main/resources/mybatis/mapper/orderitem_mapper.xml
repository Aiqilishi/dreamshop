<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mmko.dao.IOrderItemDao">


    <insert id="createOrderItem">
        insert into t_order_item(item_id,order_id,product_id,product_name,product_price,quantity)
        values(#{itemId},#{orderId},#{productId},#{productName},#{productPrice},#{quantity})
    </insert>
</mapper>