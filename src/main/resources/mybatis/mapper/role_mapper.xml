<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mmko.dao.IRoleDao">

    <resultMap id="DataMap" type="cn.mmko.po.RolePo">
        <id property="roleId" column="role_id"/>
        <result property="roleName" column="role_name"/>
        <result property="roleCode" column="role_code"/>
        <result property="description" column="description"/>
        <result property="createTime" column="create_time"/>
    </resultMap>
    <select id="queryRoleByRoleIds" resultType="java.lang.String" parameterType="java.util.List">
        select role_code from t_role where role_id in
        <foreach collection="list" item="item" index="index" open="(" close=")" separator=",">
            #{item}
        </foreach>
    </select>


</mapper>